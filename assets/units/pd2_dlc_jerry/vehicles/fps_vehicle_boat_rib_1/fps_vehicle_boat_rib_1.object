<dynamic_object>
	<diesel materials="units/pd2_dlc_jerry/vehicles/fps_vehicle_boat_rib_1/fps_vehicle_boat_rib_1" orientation_object="rp_fps_vehicle_boat_rib_1" />
	<sequence_manager file="units/pd2_dlc_jerry/vehicles/fps_vehicle_boat_rib_1/fps_vehicle_boat_rib_1" />
	<animations>
		<animation_group name="ag_speedometer" enabled="false">
				<object name="a_speedometer" />
		</animation_group>
		<animation_group name="ag_rpm_meter" enabled="false">
				<object name="a_rpm_meter" />
		</animation_group>
		<animation_group name="anim_steering_wheel_left" enabled="false">
				<object name="anim_steering_wheel_left" />
		</animation_group>
		<animation_group name="anim_steering_wheel_right" enabled="false">
				<object name="anim_steering_wheel_right" />
		</animation_group>
		<animation_group name="anim_propeller" enabled="false">
				<object name="anim_propeller" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_chassis" enabled="false" template="vehicle" friction="0.0" restitution="1" collision_script_filter="1" collision_script_quiet_time="0.5" collision_script_tag="vehicle_collision" give_standing_velocity="false">
			<object name="anim_body" />
			<object name="c_body" collision_type="convex" padding="-2.5" />
		</body>
<!-- /// Static //////////////////////////////////////////////////////////// -->
		<body name="body_static_vehicle" enabled="true" template="static" >
			<object name="c_body" collision_type="convex" />
		</body>
	</bodies>
	<effects>
		<effect_spawner
			name="es_smoke_med"
			enabled="false"
			object="v_repair_engine"
			effect="effects/payday2/particles/vehicle/vehicle_damage_small_smoke"
		/>
		<effect_spawner
			name="es_smoke_full"
			enabled="false"
			object="v_repair_engine"
			effect="effects/payday2/particles/vehicle/vehicle_damage_big_smoke"
		/>
		<!-- <effect_spawner
			name="water_01"
			enabled="false"
			object="e_water_01"
			effect="effects/payday2/particles/water/water_splash_rib_boat"
		/>
		<effect_spawner
			name="water_02"
			enabled="false"
			object="e_water_02"
			effect="effects/payday2/particles/water/water_splash_rib_boat"
		/> -->
		<effect_spawner
			name="water_engine"
			enabled="false"
			object="e_water_03"
			effect="effects/payday2/particles/water/water_speed_splash_rib_boat"
		/>
	</effects>
	<decal_surfaces default_material="rubber">
		<decal_mesh name="dm_plastic"					enabled="true" material="plastic" />
		<decal_mesh name="dm_rubber"					enabled="true" material="rubber" />
		<decal_mesh name="dm_sheet_metal"				enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_sheet_metal_motor"			enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_sheet_metal_propeller"		enabled="true" material="sheet_metal" />
	</decal_surfaces>
	<graphics>
		<graphic_group name="grp_vehicle" enabled="true">
			<lod_object name="lod_g">
				<object name="g_lod0" enabled="true" max_distance="1500" />
				<object name="g_lod1" enabled="true" max_distance="3000" />
				<object name="g_lod2" enabled="true" />
			</lod_object>
			<object name="s_s" enabled="true" shadow_caster="true" />
		</graphic_group>

		<graphic_group name="grp_icon_entry" enabled="true">
			<lod_object name="lod_icon_drive">
				<object name="g_icon_door_drive_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_drive_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_front_r_pass">
				<object name="g_icon_door_front_r_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_front_r_pass_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_rear_l_pass">
				<object name="g_icon_door_rear_l_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_rear_l_pass_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_rear_r_pass">
				<object name="g_icon_door_rear_r_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_rear_r_pass_lod1" enabled="true" />
			</lod_object>
		</graphic_group>

		<graphic_group name="grp_icon_loot" enabled="true">
			<lod_object name="lod_icon_loot">
				<object name="g_icon_loot_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_loot_lod1" enabled="true" />
			</lod_object>
		</graphic_group>

		<graphic_group name="grp_icon_repair" enabled="true">
			<lod_object name="lod_icon_repair">
				<object name="g_icon_repair_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_repair_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>

	<vehicle name="vhc_vehicle">
		<!-- IMPORTANT: suspension supported_mass must be 1/4 of chassis mass, if not vehicle behavior is undefined -->
		<!-- IMPORTANT: downforce should not go above 20, or it will compress the suspension and make the car unable to handle bumps -->
		<!-- IMPORTANT: use inertia multipliers with care -->
		<chassis body="body_chassis" mass="1500" center_of_mass_offset="0.0 -0.5 0.5" inertia_multiplier="1.0 1.1 1.0" downforce_strength="1.0" drag_strength="0.1"/>
		<wheels>
			<wheel spin_object="anim_tire_front_left"		turn_object=""	radius="0.35" width="0.25" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="30" braking_torque="100" handbrake_torque="0" />
			<wheel spin_object="anim_tire_front_right"		turn_object=""	radius="0.35" width="0.25" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="30" braking_torque="100" handbrake_torque="0" />
			<wheel spin_object="anim_tire_rear_left"		turn_object=""	radius="0.35" width="0.25" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="30" braking_torque="100" handbrake_torque="0" />
			<wheel spin_object="anim_tire_rear_right"		turn_object=""	radius="0.35" width="0.25" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="30" braking_torque="100" handbrake_torque="0" />
		</wheels>
		<suspension max_compression="0.1" max_extension="0.1" spring_strength="55000" damper_rate="3000" supported_mass="375" />
		<gearbox switch_time="0.01" ratio_multiplier="4.0">
			<ratios>
				<ratio value="-3.0" />
				<ratio value="0.0" />
				<ratio value="4.75" />
				<ratio value="3.75" />
				<ratio value="2.75" />
			</ratios>
		</gearbox>
		<engine max_torque="400" max_rpm="2500"/>
	</vehicle>

</dynamic_object>
